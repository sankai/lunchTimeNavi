<h1>Listing launch_coments</h1>

<table>
  <tr>
    <th>Launch coment text</th>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @launch_coments.each do |launch_coment| %>
  <tr>
    <td><%= launch_coment.launch_coment_text %></td>
    <td><%= launch_coment.name %></td>
    <td><%= link_to 'Show', launch_coment %></td>
    <td><%= link_to 'Edit', edit_launch_coment_path(launch_coment) %></td>
    <td><%= link_to 'Destroy', launch_coment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

